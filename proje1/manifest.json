{
  "manifest_version": 3,
  "name": "Kişisel Not Defterim",
  "version": "1.1",
  "description": "Tarayıcıda gezinirken pratik bir not defteri kullanın.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html",
